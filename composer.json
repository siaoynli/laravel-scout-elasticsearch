{
  "name": "siaoynli/laravel-scout-elasticsearch",
  "description": "Elastic Driver for Laravel Scout",
  "keywords": [
    "laravel",
    "scout",
    "elasticsearch",
    "elastic"
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/scout": "^5.0|^6.0|^7.0",
    "elasticsearch/elasticsearch": "^7.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Siaoynli\\ScoutEngines\\Elasticsearch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Siaoynli\\ScoutEngines\\Elasticsearch\\Test\\": "tests"
    }
  },
  "suggest": {
    "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^5.0)."
  },
  "extra": {
    "laravel": {
      "providers": [
        "Siaoynli\\ScoutEngines\\Elasticsearch\\ElasticsearchProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
